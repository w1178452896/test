<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taylorsfan.blog.repository.relation.SortBlogMapper">
    <resultMap id="BaseResultMap" type="com.taylorsfan.blog.model.relation.SortBlog">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="blog_id" jdbcType="INTEGER" property="blogId"/>
        <result column="sort_id" jdbcType="INTEGER" property="sortId"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, blog_id, sort_id
    </sql>
    <insert id="insert" parameterType="com.taylorsfan.blog.model.relation.SortBlog" useGeneratedKeys="true"
            keyProperty="id">
        insert into sort_blog (blog_id, sort_id
        )
        values ( #{blogId,jdbcType=INTEGER}, #{sortId,jdbcType=INTEGER}
        )
    </insert>
    <select id="count" parameterType="integer" resultType="integer">
        select count(*) from sort_blog WHERE sort_id = #{sortId}
    </select>
    <delete id="deleteByBlogId" parameterType="integer">
        DELETE FROM sort_blog WHERE blog_id = #{blogId}
    </delete>
    <delete id="deleteBySortId" parameterType="integer">
        DELETE FROM sort_blog WHERE sort_id = #{sortId}
    </delete>
</mapper>